{
  "name": "vercel-monorepo",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "packageManager": "pnpm@7.24.2",
  "dependencies": {
    "lerna": "5.6.2"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.18.6",
    "@babel/core": "^7.20.2",
